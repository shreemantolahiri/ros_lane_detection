<launch>
    <!--   ebot -->
        <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find autoz_task)/urdf/ebot.xacro'"/>

    <!-- empty world-->    
        <include file="$(find autoz_task)/launch/my_world.launch" />
    

        <arg name="x" default="0" />
        <arg name="y" default="0" />
        <arg name="z" default="0" />



    <!-- Spawn the robot model -->
    <node name="mybot_spawn" pkg="gazebo_ros" type="spawn_model" output="screen" 
        args="-urdf -param robot_description -model ebot -x $(arg x) -y $(arg y) -z $(arg z)" />






</launch>
